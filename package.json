      "prerelease": "npm run lint && npm run typecheck && npm run build"
    }
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
