    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "9.0.0",
    "tslint-plugin-prettier": "2.1.0",
    "tslint-react": "4.1.0",
    "typescript": "3.7.4"
  },
  "keywords": [
    "babel",
    "breakpoint",
    "matchMedia",
    "react",
    "responsive",
    "storybook",
    "typescript",
    "viewport",
