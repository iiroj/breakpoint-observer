    "commitlint": "8.2.0",
    "husky": "3.1.0",
    "lint-staged": "10.0.0-beta.12",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rollup": "1.27.14",
    "rollup-plugin-commonjs": "10.1.0",
