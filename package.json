{
  "name": "breakpoint-observer",
  "version": "1.5.18",
  "description": "A React Component for rendering based on breakpoints",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi (https://iiro.fi)",
  "license": "MIT",
  "homepage": "https://github.com/iiroj/breakpoint-observer",
